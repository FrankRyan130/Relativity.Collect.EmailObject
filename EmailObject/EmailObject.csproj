<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Copyright>VerQu</Copyright>
    <Version>1.1.19</Version>
    <Description>Email, EML, MSG, EV data object classes</Description>
    <Company>VerQu</Company>
    <Authors>VerQu</Authors>
    <Platforms>AnyCPU;x64</Platforms>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
    <IncludeSymbols>true</IncludeSymbols>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitLab" Version="1.1.1" PrivateAssets="All" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="EMLForge" Version="1.2.11" />
    <PackageReference Include="MSGForge" Version="1.2.18" />
    <PackageReference Include="VerQu.Core" Version="1.1.47" />
  </ItemGroup>
</Project>